{
    "[json][rust]": {
        "coc.preferences.formatOnSave": true
    },
    "dart.showTodos": false,
    "rust-analyzer.cargo.extraEnv": {
        "SKIP_BUILDSCRIPT": "1"
    },
    "rust-analyzer.check.extraArgs": [
        "--target-dir",
        "/tmp/rust-analyzer-check"
    ],
    "rust-analyzer.diagnostics.disabled": [
        "inactive-code"
    ],
    "suggest.noselect": true
}
